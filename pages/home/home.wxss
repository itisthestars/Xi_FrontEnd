page {
  background-color:white;
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

swiper {
  height: 400rpx;
  width: 100vw;
  line-height: 30rpx;
  margin: auto;
  
}

swiper-item navigator image {
  width: 100vw;
  border-radius: 100rpx;

}

